                            <div class="social-links">
                                <ul class="social">
                                    <li><a href="#"><i class="fab fa-facebook-f"></i></a></li>
                                    <li><a href="#"><i class="fab fa-twitter"></i></a></li>
                                    <li><a href="#"><i class="fab fa-behance"></i></a></li>
                                    <li><a href="#"><i class="fab fa-instagram"></i></a></li>
                                </ul>
                            </div>

                        </div>
                    </div>
                </div>

                <div class="col-lg-3 col-md-6 col-12">
                    <div class="single-team">

                        <div class="image">
                            <img src="~/content/assets/img/team/3.jpg" alt="#">
                        </div>

                        <div class="info-head">

                            <div class="info-box">
                                <h4 class="name"><a href="#">Dawn Jenkins</a></h4>
                                <span class="designation">Web Developer</span>
                            </div>


                            <div class="social-links">
                                <ul class="social">
                                    <li><a href="#"><i class="fab fa-facebook-f"></i></a></li>
                                    <li><a href="#"><i class="fab fa-twitter"></i></a></li>
                                    <li><a href="#"><i class="fab fa-behance"></i></a></li>
                                    <li><a href="#"><i class="fab fa-instagram"></i></a></li>
                                </ul>
                            </div>

                        </div>
                    </div>
                </div>

                <div class="col-lg-3 col-md-6 col-12">
                    <div class="single-team">

                        <div class="image">
                            <img src="~/content/assets/img/team/4.jpg" alt="#">
                        </div>

                        <div class="info-head">

                            <div class="info-box">
                                <h4 class="name"><a href="#">Darla Brown</a></h4>
                                <span class="designation">SEO Expert</span>
                            </div>


                            <div class="social-links">
                                <ul class="social">
                                    <li><a href="#"><i class="fab fa-facebook-f"></i></a></li>
                                    <li><a href="#"><i class="fab fa-twitter"></i></a></li>
                                    <li><a href="#"><i class="fab fa-behance"></i></a></li>
